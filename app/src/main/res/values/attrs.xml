<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ItemTextArrowView">
        <attr name="itemTitle" format="string" />
        <attr name="itemContent" format="string" />
        <attr name="showArrow" format="boolean" />
        <attr name="itemColor" format="color"/>
        <attr name="contentColor" format="color" />
    </declare-styleable>
    <declare-styleable name="TwoTextView">
        <attr name="title" format="string" />
        <attr name="value" format="string" />
        <attr name="titleTextColor" format="color" />
        <attr name="valueTextColor" format="color" />
    </declare-styleable>
</resources>